{
  "extends": [
    "egg"
  ],
  "globals": {
    "$": true,
    "jest": true,
    "expect": true,
    "beforeAll": true,
    "afterAll": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-bitwise": [
      0
    ],
    "experimentalDecorators": [
      0
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}
